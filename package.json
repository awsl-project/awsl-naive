{
  "name": "awsl-naive",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^10.4.1",
    "lyla": "^1.2.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@types/node": "^20.6.0",
    "@types/web-bluetooth": "^0.0.17",
    "@vicons/antd": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "naive-ui": "^2.34.4",
    "prettier": "3.0.3",
    "typescript": "^5.2.2",
    "vfonts": "^0.0.3",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.10"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.tsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --parser=vue --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write --parser markdown --prose-wrap never",
      "eslint --fix"
    ]
  }
}